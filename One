local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/2lwg1cdpiW/eDZzpHyBzi/refs/heads/main/orion2.txt')))()

local Window = OrionLib:MakeWindow({
    Name = "We changed the script path", 
    HidePremium = false, 
    SaveConfig = true, 
    ConfigFolder = "OrionTest"
})

local Tab = Window:MakeTab({
	Name = "Tab 1",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local useClipboard = setclipboard or toclipboard or set_clipboard or (Clipboard and Clipboard.set)

Tab:AddButton({
	Name = "Youtube link[Option 1]",
	Callback = function()
      		useClipboard("https://youtu.be/AfbOqUmm-Dk?si=2NGYRQZgkM1zzXVu")
  	end    
})


Tab:AddButton({
	Name = "Discord link[Option 2]",
	Callback = function()
      		useClipboard("https://discord.gg/Msc93QtzM3")
  	end    
})
